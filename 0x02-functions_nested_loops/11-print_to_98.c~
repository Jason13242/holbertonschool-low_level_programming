#include"main.h"
#include<stdio.h>

/**
 * print_to_98 - prints all natural numbers
 *               from n to 98
 * Returns: 0;
 */

void print_to_98(int n)
{
	if (n <= 98)
	{
		int e;
/*
 * e - variable used for equivalence.
 */
		for (e = n ; e <= 98 ; e++)
		{
			printf("%d", e);
			printf(", ");
		}
	}
	if (n > 98)
	{
		int e;

		for (e = n; e > 98; e--)
		{
			prinf("%d", e);
			printf(", ");
		}
	}
	printf("98");
	printf("\n");
}

#include"main.h"

/**
 *  _islower - function that prints
 *             that checks if a char
 *             is upper or lowecase
 * @c: this variable is the input.
 *Return: 1 if c lowercase, 0 if upper.
 */

/*
 * c - this variable integer is the input
 *      number to be analized.
 */

int _islower(int c)
{
/*
 *c - this variable integer is the input
 *   number to be analized.
 */
	if (c >= 97 && c <= 122)
	{
		return (1);
	}
	else
	{
		return (0);
	}
}

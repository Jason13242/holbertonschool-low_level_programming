/*este codigo incluye las librerias necesarias*/
#include <stdlib.h>
#include <time.h>
#include<stdio.h>

/*
 *funcion que determina si el numero propuesto es
 * mayor,
 * menor
 * o igual a cero
 */

int main(void)
{

  int n;

  srand(time(0));
  n = rand() - RAND_MAX / 2;
  /* your code goes there */
  if (n < 0)
      printf("%d is negative\n", n);
  else if (n == 0)
    printf("%d is zero\n", n);
  else 
  printf("%d is positive\n", n);


  return (0);
}
